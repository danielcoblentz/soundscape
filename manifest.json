{
    "manifest_version": 3,
    "name": "Soundscape",
    "version": "1.0",
    "permissions": [
        "tabs",
        "webNavigation",
        "<all_urls>"
      ],
    "action": {
        "default_popup": "popup.html",
        "default_icon": "icon.png"
    },
    "background": {
        "service_worker": "background.js"
    },
    "icons": {
        "48": "icon.png"
    }
}
